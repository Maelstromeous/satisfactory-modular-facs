{
  "rewrites": [
    { "source": "/graph", "destination": "/" },
    { "source": "/recipes", "destination": "/" },
    { "source": "/share/:path*", "destination": "/" }
  ],
  "headers": [
    {
      "source": "/assets/game/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=704800, immutable"
        }
      ]
    },
    {
      "source": "/js/fa/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}